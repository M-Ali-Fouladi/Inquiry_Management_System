<div>
    <label id="Grid_kala1">gridkala</label>
    @(Html.Kendo().Grid<CustomMembershipEF.Models.InquiryViewModel>()
                .Name("Grid_kala")
               
                .Columns(columns =>
                {
                    columns.Bound(c => c.Id).Hidden(true);
                    columns.Bound(c => c.Inquiry_Element_Code).Hidden(true);
                    columns.Bound(c => c.Element_count).Title("تعداد درخواست");
                    columns.Bound(c => c.Inquiry_Element).Title("عنوان کالا");
                })
                
                 .Pageable(pageable => pageable
                .Refresh(true)
                .PageSizes(true)
                .ButtonCount(5))
                .Sortable()
                .Scrollable()
                  .HtmlAttributes(new { style = "height:550px;" })
                .DataSource(dataSource => dataSource
                .Ajax()
                .PageSize(10)
                .Read(read => read.Action("Select_Report_kala", "Home"))
                )
    )

</div>

