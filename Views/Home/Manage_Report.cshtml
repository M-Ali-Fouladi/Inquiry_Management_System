<div>
    <label id="Salary_Grid1">Salary_Grid</label>

    @(Html.Kendo().Grid<CustomMembershipEF.Models.Test_Inquiry_Viewmodel>()
                .Name("Salary_Grid")
                .AutoBind(false)
                .Columns(columns =>
                {
                    columns.Bound(c => c.Id).Hidden(true);
                    columns.Bound(c => c._Inquiry_DateYear).Title("سال ");
                         //.Filterable(filterable => filterable.UI("DateYear"));
                    columns.Bound(c => c._Inquiry_DateMonth).Title("ماه");
                    columns.Bound(c => c.Test_Inquiry_Coast).Title("درآمد پیش بینی شده به ریال");

                })
                .Pageable(pageable => pageable
                .Refresh(true)
                .PageSizes(true)
                .ButtonCount(5))
                .Sortable()

                .Scrollable()
                  .HtmlAttributes(new { style = "height:550px;" })
                   .Filterable(filterable => filterable
        .Extra(false)
        .Operators(operators => operators
            .ForString(str => str.Clear()
                .StartsWith("شروع شود با")
                .IsEqualTo("برابر باشد با")
                .IsNotEqualTo("مخالف باشد با")
                .Contains("شبیه باشد با")
            ))
        )
                .DataSource(dataSource => dataSource
                .Ajax()
                .PageSize(10)
            .Read(read => read.Action("Select_Report_Salary", "Home"))
                )
    )
